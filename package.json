{
  "name": "messenger",
  "private": true,
  "version": "1.0.0",
  "description": "NestJs Websocket",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "docker:start:dev": "docker run -p 4000:4000 messenger-api",
    "docker:test:build:dev": "docker build -t nestjs-api:test --target=create-build .",
    "docker:test:start:dev": "docker run nestjs-api:test sh -c 'npm run test'",
    "start:prod": "cross-env NODE_ENV=prod node dist/src/api/v1/main.js",
    "doc:api": "node dist/swagger",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "knex": "knex",
    "migrate": "npx knex migrate:latest",
    "migrate:up": "npx knex migrate:up",
    "migrate:down": "npx knex migrate:down",
    "migrate:rollback": "npx knex migrate:rollback",
    "migrate:rollback:all": "knex migrate:rollback --all",
    "knex:prod": "knex --knexfile dist/knexfile.js",
    "knex:prod:migrate:latest": "npm run knex:prod migrate:latest",
    "knex:prod:migrate:up": "npm run knex:prod migrate:up",
    "knex:prod:migrate:down": "npm run knex:prod migrate:down",
    "knex:prod:migrate:rollback": "npm run knex:prod migrate:rollback",
    "knex:prod:migrate:rollback:all": "npm run knex:prod migrate:rollback --all",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.0.5",
    "@nestjs/graphql": "^11.0.6",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.4.3",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.15",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/websockets": "^9.4.3",
    "@types/socket.io": "^3.0.2",
    "awaity": "^1.0.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "chai": "^4.3.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.8",
    "dotenv": "^16.1.4",
    "express-rate-limit": "^6.9.0",
    "find-config": "^1.0.0",
    "gridfs-stream": "^1.1.1",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-service": "^10.0.4",
    "knex": "^3.0.1",
    "moment": "^2.29.4",
    "mongoose": "^7.2.4",
    "nestjs-knex": "^2.0.0",
    "nestjs-pino": "^3.3.0",
    "nestjs-zod": "^3.0.0",
    "node_extra_ca_certs_mozilla_bundle": "^1.0.5",
    "objection": "^3.1.2",
    "pg": "^8.11.3",
    "pino": "^8.14.1",
    "pino-http": "^8.4.0",
    "pino-pretty": "^10.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "serve-static": "^1.15.0",
    "socket.io": "^4.6.2",
    "socket.io-client": "^4.7.1",
    "uuid": "^9.0.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/chai": "^4.3.5",
    "@types/express": "^4.17.13",
    "@types/gridfs-stream": "^0.5.37",
    "@types/jest": "^28.1.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.3",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
